@model Car

@{
    ViewData["Title"] = "CreateCar";
}

<h1>CreateCar</h1>


@using (Html.BeginForm("CreateCar", "Car", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
	<div class=" form-group row">
		@Html.LabelFor(m => m.Num)
		@Html.EditorFor(model => model.Num, new { htmlAttributes = new { @class = "form-control"} })
	</div>

	<div class=" form-group row">
		@Html.LabelFor(m => m.Model)
		@Html.EditorFor(model => model.Model, new { htmlAttributes = new { @class = "form-control"} })
	</div>

	<div class=" form-group row">
		@Html.LabelFor(m => m.Color)
		@Html.EditorFor(model => model.Color, new { htmlAttributes = new { @class = "form-control"} })
	</div>

	<div class=" form-group row">
		@Html.LabelFor(m => m.Manufacture)
		@Html.EditorFor(model => model.Manufacture, new { htmlAttributes = new { @class = "form-control"} })
	</div>

	<div class=" form-group row">
		@Html.LabelFor(m => m.ImgUrl)
		<input type="file" name="Img" />
	</div>

	
	<br />

	<input type="submit" value="Submit" class="btn btn-primary" />
}





