
@{
    ViewData["Title"] = "CreateCar";
}

<h1>CreateCar</h1>


<form method="post" action="/Car/SaveCreateCar" enctype="multipart/form-data">

	<div class=" form-group row">
		<label for="Num">Car Number</label>
		<input type="text" class="form-control" name="Num" placeholder="Number" /><br />
	</div>

	<div class=" form-group row">
		<label for="Model">Model</label>
		<input type="text" class="form-control" name="Model" placeholder="Model" />
	</div>

	<div class=" form-group row">
		<label for="Color">Color</label>
		<input type="text" class="form-control" name="Color" placeholder="Color" />
	</div>

	<div class=" form-group row">
		<label for="Manufacture">Manufacture</label>
		<input type="text" placeholder="Manufacture" name="Manufacture" class="form-control" />
	</div>
	<div class=" form-group row">
		<label for="Img">Image</label>
		<input type="file" name="Img" />
	</div>
	<br />
	<input type="submit" value="Submit" class="btn btn-primary" />


</form>




